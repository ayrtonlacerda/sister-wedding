datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Guests {
  id             Int      @id @default(autoincrement())
  createdAt      DateTime @default(now())
  email          String   @unique
  name           String
  celphone       Int
  amountChildren Int?
}
